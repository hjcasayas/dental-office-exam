{
  "name": "@dental/source",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:rest-api": "npm run dev -w @dental/rest-api --if-present",
    "dev:features": "npm run dev -w @dental/features --if-present",
    "dev:implementations": "npm run dev -w @dental/implementations --if-present",
    "test:features": "npm run test -w @dental/features --if-present",
    "prettier:write:all": "npm run prettier:write --workspaces --if-present",
    "eslint:fix:all": "npm run eslint:fix --workspaces --if-present",
    "git:commit": "npm run prettier:write:all && npm run eslint:fix:all && cz"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
